<h1>编辑个人资料</h1>

<% form_for(@profile,:url => resume_profile_path(@resume),:html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form',:locals => { :f => f } %>    
  <p>
    <%= f.submit '更新' %>
  </p>
<% end %>
<%= link_to '返回', edit_resume_path(@resume) %>