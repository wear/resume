<h2>投简历</h2>
<% form_tag( do_send_resume_path(@resume),:method => :post) do %>
  <p><%= label_tag 'email' %> <br /><%= text_field_tag :email,nil,:size => 32 %></p>
  <p><%= label_tag '职位' %> <br /><%= text_field_tag :job,nil,:size => 32 %></p>
  <p><%= label_tag '内容' %> <br /><%= text_area_tag :content,nil,:rows => 12, :cols => 65
 %></p> 
 <p>
	<%= submit_tag '发送' %>
 </p>
<% end -%>