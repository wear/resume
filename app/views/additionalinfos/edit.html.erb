<% content_for :tabs do -%> 
	<li><%=link_to(t(:edit_additionalinfo),'#tabs-content') %></li> 
    <li><%=link_to t(:back), edit_resume_path(@resume)%></li> 
<% end -%>

<% form_for(@additionalinfo,:url => resume_additionalinfo_path(@resume)) do |f| %>
  <%= f.error_messages %>
    <%= render :partial => 'form',:locals => { :f => f } %>
  <p>
    <%= f.submit t(:update) %>
  </p>
<% end %>
