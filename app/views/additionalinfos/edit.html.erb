<% content_for :tabs do -%> 
 	<li><%=link_to('全部简历', resumes_path ) %></li>
	<li ><%=link_to(@resume.name,edit_resume_path(@resume)) %></li>
	<li ><%=link_to('编辑工作经验',resume_positions_path(@resume)) %></li>
	<li class='ui-tabs-selected'><%=link_to('编辑兴趣爱好','#tabs-content') %></li> 
    <li><%=link_to('编辑教育情况',resume_educations_path(@resume)) %></li>
<% end -%>

<% form_for(@additionalinfo,:url => resume_additionalinfo_path(@resume)) do |f| %>
  <%= f.error_messages %>
    <%= render :partial => 'form',:locals => { :f => f } %>
  <p>
    <%= f.submit '更新' %>
  </p>
<% end %>
