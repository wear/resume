<% content_for :tabs do -%> 
 	<li><%=link_to(t(:new_education), '#tabs-content' ) %></li>
    <li><%=link_to(t(:back),resume_educations_path(@resume)) %></li>
<% end -%>

<% form_for(@education,:url => resume_educations_path(@resume)) do |f| %>
  <%= f.error_messages %>
   <%= render :partial => 'form', :locals => { :f => f }%> 
  <p>
    <%= f.submit t(:create) %>
  </p>
<% end %>
