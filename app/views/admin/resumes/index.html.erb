<table>
  <tr> 
	<th>id</th>
	<th>名称</th>
	<th>完成度</th>
	<th>创建时间</th>  
  </tr>

<% @resumes.each do |resume| %>
  <tr>         
	<td><%= resume.id %></td>
	<% completeness ||= resume_completeness(resume) %>
	<% percent = completeness[:score] %>
	<td> <%=link_to resume.name,resume.public_url,:target =>'_blank' %></td>
	<td><%= number_to_percentage percent.to_i %></td>
		<td><%= resume.created_at %></td>     
  </tr>
<% end %>
</table>