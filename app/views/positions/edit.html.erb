<% content_for :tabs do -%> 
 	<li><%=link_to('全部简历', resumes_path ) %></li>
	<li ><%=link_to(@resume.name,edit_resume_position_path(@resume)) %></li>
	<li class='ui-tabs-selected'><%=link_to('编辑工作经验','#tabs-content') %></li>
	<li><%=link_to('编辑兴趣爱好',edit_resume_additionalinfo_path(@resume)) %></li> 
    <li><%=link_to('编辑教育情况',edit_resume_education_path(@resume)) %></li>
<% end -%>
<div class='span-13'> 

<% form_for(@position, :url => resume_position_path(@resume,@position)) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f }%>
  <p>
    <%= f.submit '更新' %>   <%=link_to '返回', resume_positions_path(@resume)%>
  </p>
<% end %> 
</div>
