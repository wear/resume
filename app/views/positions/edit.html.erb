<% content_for :tabs do -%> 
 	<li><%=link_to(t(:edit_positions),'#tabs-content') %></li> 
    <li><%=link_to t(:back), resume_positions_path(@resume)%></li>       
    <li><%=link_to @resume.name, edit_resume_path(@resume)%></li>
<% end -%>
<div class='span-13'> 

<% form_for(@position, :url => resume_position_path(@resume,@position)) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f }%>
  <p>
    <%= f.submit '更新' %> 
  </p>
<% end %> 
</div>
